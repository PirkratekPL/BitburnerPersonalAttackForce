{
    "include": [
        "./src/bin/**/*.ts",
        "./src/bin/bitburner.ts"
    ],
    "typeAcquisition": {
        "include": [
            "./src/bin/bitburner.ts"
        ]
    },
    "compilerOptions": {
        "jsx": "react",
        "target": "ES2021",
        "module": "ESNext",
        "rootDir": "src/",
        "baseUrl": "src/",
        "outDir": "build/",
        "paths": {
            "/bitburner": ["./bin/bitburner.ts"]
        },
        "moduleResolution": "node",
        "strictNullChecks": true,
        "strictPropertyInitialization": true,"declaration": false,
        "strictBindCallApply": true,
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "preserveConstEnums": true
        }
}